<div>
  <%= render partial: "shared/profile"%>
</div>
<div>
  <ul class="nav nav-tabs">
    <li class="nav-item">
      <%= link_to "My Post", user_path(@user), class: "nav-link active"%>
    </li>
    <li class="nav-item">
      <%= link_to "My Comment", comments_user_path(@user), class: "nav-link"%>
    </li>
    <li class="nav-item">
      <%= link_to "My Collect", collects_user_path(@user), class: "nav-link"%>
    </li>
    <li class="nav-item">
      <%= link_to "My Draft", drafts_user_path(@user), class: "nav-link"%>
    </li>
    <li class="nav-item">
      <%= link_to "My Friend", friends_user_path(@user), class: "nav-link"%>
    </li>
  </ul>
</div>
<table class="table table-hover">
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td class="text-center"><%= link_to post.title, post_path(post)%></td>
        <td class="text-center">Published</td>
        <td class="text-center"><%= post.created_at.strftime("%F")%></td>
      </tr>
    <%end%>
  </tbody>
</table>
<div>
  <%= link_to "New Post", new_post_path, class: "btn btn-primary"%>
</div>