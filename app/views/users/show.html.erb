<%= @user.name%>
<%= image_tag @user.check_avatar(@user)%>
<%= @user.introduction%>

<%= link_to "My Post", user_path(@user)%>
<%= link_to "My Comment", comments_user_path(@user)%>
<%= link_to "My Draft", drafts_user_path(@user)%>


<% @posts.each do |post|%>
<%= link_to post.title, post_path(post)%>
<%= post.created_at%>
<%end%>

<%= link_to "New Post", new_post_path%>